#include <strings.h>
#include <stdlib.h>

int	isnum(char *str)
{
	while (*str)
	{
		if (47 < *str && *str < 58)
			str++;
		else
			return (0);
	}
	return (1);
}

int main(int ac, char *av[])
{
	int index;
	int sum;
	int res[3];

	if (ac != 3)
		return (0);
	if (2 != (isnum(av[1]) + isnum(av[2])))
		return (0);
	for (i = 0; i <= 100; i++)
	{
		
